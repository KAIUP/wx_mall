.swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper image {
  width: 100%;
}
.cate {
  display: flex;
}
.cate navigator {
  flex: 1;
  padding: 20rpx;
}
.cate image {
  width: 100%;
}
.floor .floor_group .floor_title {
  font-size: 0;
}
.floor .floor_group .floor_title image {
  width: 100%;
}
.floor .floor_group .floor_cont {
  overflow: hidden;
  padding: 10rpx 10rpx;
}
.floor .floor_group .floor_cont navigator {
  font-size: 0;
  float: left;
  width: 33.33%;
}
.floor .floor_group .floor_cont navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
  border-bottom: 10rpx solid #fff;
}
.floor .floor_group .floor_cont navigator image {
  width: 100%;
  height: 100%;
}
